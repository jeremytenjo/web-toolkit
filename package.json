{
  "name": "@tenjojeremy/web-toolkit",
  "version": "6.91.0",
  "repository": "git://github.com/tenjojeremy/web-toolkit.git",
  "description": "Library of reusable UI and utilities",
  "publisher": "Jeremy Tenjo",
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "wapp": "index.js"
  },
  "scripts": {
    "wapp:test": "node index.js start test",
    "start": "start-storybook -p 9002 -c .storybook -s .storybook/public --quiet",
    "deploy": "npm run lint:check && npm run cy:run-ci && npm run deploy:firebase && npm run publish:library && git push",
    "deploy:quick": "npm run deploy:firebase && npm run publish:library && git push",
    "build:library": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ./.babelrc ./components --out-dir ./components/.build/ --ignore 'components/**/specs','components/**/stories' ",
    "build:storybook": "build-storybook -c .storybook -o .storybook/.build -s public",
    "deploy:firebase": "npm run build:storybook && firebase deploy",
    "publish:library": "npm run build:library && npm version minor && cp package.json ./build && cp .npmrc ./build && cd build && npm publish && cd ../ && rmdir /s /q build",
    "test": "jest",
    "prettify": "prettier \"**/*.js\" --write",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "cy:run-ci": "start-server-and-test start http://localhost:9002 cy:run",
    "lint:fix": "eslint ./components --fix",
    "lint:check": "eslint ./components",
    "check-staged": "lint-staged",
    "new:component": "node .storybook/generators/component/index.js"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-throw-expressions": "^7.8.3",
