{
  "name": "@tenjo/web-features",
  "version": "1.63.0",
  "main": "build/index.js",
  "files": ["build/index.js"],
  "license": "MIT",
  "scripts": {
    "start": "start-storybook -p 9002 -c .storybook -s public --quiet",
    "test": "jest",
    "deploy": "gpush",
    "build:library": "yarn webpack-scripts build:library",
    "publish": "yarn webpack-scripts build:library && yarn publish --minor && gpush && yarn build:storybook && firebase deploy",
    "build:storybook": "build-storybook -c .storybook -o .storybook/storybook-build -s public",
    "deploy:firebase": "firebase deploy --token=$FIREBASE_DEPLOY_TOKEN",
    "pretty": "prettier \"**/*.js\" --write",
    "check-staged": "lint-staged",
    "lint:fix": "eslint ./src --fix",
    "lint:check": "eslint ./src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "yarn lint:check",
      "prettier --write",
      "prettier --list-different",
      "jest --passWithNoTests"
    ]
  },
  "dependencies": {
    "@babel/plugin-syntax-throw-expressions": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@material-ui/core": "^4.0.0",
    "@storybook/react": "^5.0.11",
    "@tenjo/eslint-config": "^0.18.0",
    "@tenjo/webpack-config": "^3.76.0",
    "babel-loader": "^8.0.5",
    "date-fns": "^1.30.1",
    "ejs-lint": "^0.3.0",
    "firebase": "^6.0.4",
    "husky": "^2.3.0",
    "image-compressor.js": "^1.1.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.4",
    "react-lazyload": "^2.5.0",
    "react-scripts": "^3.0.0",
    "storybook-readme": "^5.0.3",
    "styled-components": "^4.2.0",
    "webpack-node-externals": "^1.7.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "firebase-tools": "^6.10.0",
    "jest": "^24.8.0",
    "webpack-cli": "^3.3.1"
  }
}
