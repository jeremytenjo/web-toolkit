    "deploy": "yarn webpack-scripts build:library && yarn publish --minor && gpush",
    "build:storybook": "build-storybook -c .storybook -o .storybook-build",
    "deploy:storybook": "yarn build:storybook && firebase deploy && gpush",
    "hygen": "set HYGEN_TMPLS=.hygen/_templates&& hygen",
    "hygen:mac": "HYGEN_TMPLS=.hygen/_templates hygen",
    "pretty": "prettier \"**/*.js\" --write",
    "check-staged": "lint-staged",
    "lint": "eslint ./features"
  },
  "dependencies": {
    "@babel/plugin-syntax-throw-expressions": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@material-ui/core": "^3.9.3",
    "@storybook/react": "^5.0.11",
    "@tenjo/eslint-config": "^0.18.0",
    "@tenjo/forms-input": "^0.9.0",
    "@tenjo/webpack-config": "^3.74.0",
    "babel-loader": "^8.0.5",
    "date-fns": "^1.30.1",
    "ejs-lint": "^0.3.0",
    "hygen": "latest",
    "image-compressor.js": "^1.1.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.4",
    "react-scripts": "^3.0.0",
    "storybook-readme": "^5.0.3",
