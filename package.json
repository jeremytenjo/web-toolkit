{
  "name": "web-features",
  "version": "1.0.0",
  "scripts": {
    "start": "start-storybook -p 9001 -c .storybook --quiet",
    "build": "build-storybook -c .storybook -o .storybook-build",
    "deploy": "yarn build && firebase deploy && gpush",
    "pretty": "prettier \"**/*.js\" --write",
    "hygen": "set HYGEN_TMPLS=.hygen/_templates&& hygen",
    "hygen-mac": "HYGEN_TMPLS=.hygen/_templates hygen"
  },
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@storybook/react": "^5.0.1",
    "@tenjo/eslint-config": "^0.0.1",
    "babel-loader": "^8.0.5",
    "ejs-lint": "^0.3.0",
    "hygen": "latest",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "^2.1.8",
    "storybook-readme": "^4.0.5",
    "styled-components": "^4.1.3",
    "yup": "^0.26.10"
  }
}
